{
  "version": 2,
  "builds": [
    {
      "src": "src/index.html",
      "use": "@vercel/parcel",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/.*",
      "dest": "/index.html",
      "continue": true
    }
  ]
}